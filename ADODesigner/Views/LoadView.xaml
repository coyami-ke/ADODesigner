<Window x:Class="ADODesigner.Views.LoadView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADODesigner.Views"
        mc:Ignorable="d"
        Title="ADODesigner by Coyami-Ke" Height="450" Width="800" Background="#FF121215" WindowStyle="None" ShowInTaskbar="False" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid>
        <TextBlock Text="ADODesigner" Foreground="{DynamicResource TextColor2}" Margin="10" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <TextBlock Text="You're using Alpha version." x:Name="textBlockProgress" Foreground="{DynamicResource TextColor}" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,410,0,0" FontSize="20"/>
        <ProgressBar x:Name="mainProgressBar" IsIndeterminate="True" Foreground="LightSkyBlue" BorderBrush="#FF111114" Background="#FF111114" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="40" Height="40" Width="500"/>
    </Grid>
</Window>
